ImGui::SliderFloat(&quot;float&quot;, &amp;f, 0.0f, 1.0f);"><pre><span class="pl-en">ImGui::Text</span>(<span class="pl-s"><span class="pl-pds">"</span>Hello, world %d<span class="pl-pds">"</span></span>, <span class="pl-c1">123</span>);
